#include <bits/stdc++.h>
using namespace std;

int f(int n) {
  if (n <= 9) {  //1～9は和をとらないから除外している。なるほど。
    return n;
  } else {
    int q = n / 10;
    int r = n % 10;  //割10だと2桁までしか対応してなくない？いいの？
    return r + f(q);  //他でも割10なので自分が問題を誤認しているだけかも。。。
  }
}

int main() {
  int N, A, B;
  cin >> N >> A >> B;
  
  int ans = 0;
  for (int n = 1; n <= N; n++) {
    int sum = f(n);
    if ((A <= sum) && (sum <= B)) {
      ans += n;
    }
  }
  cout << ans << endl;
  return 0;
}
