#include <bits/stdc++.h>
using namespace std;

int f(int n) {
  if (n <= 9) {
    return n;
  } else {
    int q = n / 10;
    int r = n % 10;
    return r + f(q);
  }
}

int main() {
  int N, A, B;
  cin >> N >> A >> B;
  
  int ans = 0;
  for (int n = 1; n <= N; n++) {
    int sum = f(n);
    if ((A <= sum) && (sum <= B)) {
      ans += n;
    }
  }
  cout << ans << endl;
  return 0;
}
