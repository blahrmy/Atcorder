#include<bits/stdc++.h>
using namespace std;
int a, b, c;
int n, cnt;
int main()
{
cin>>a>>b>>c;
cin>>n;
	
	for(int i=0; i<=a; i++)	//500*iは理解するがaとiは一致しないじゃねえか　怒（i=aか失礼失礼）
	{
		for(int j=0; j<=b; j++)
		{
			for(int k=0; k<=c; k++)
			{
				if(i*500+j*100+k*50==n) cnt++;
			}
		}
	}
	
	cout<<cnt;
	
	
	
	
	
	
	return 0;
}
