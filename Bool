#include <bits/stdc++.h>
using namespace std;

bool divide(vector<int> A) {
  for (int i = 0; i < A.size(); i++) {
    if (A.at(i) % 2 != 0) {
      return false;
    }
  }
  return true;
}

int main() {
  int N, count = 0;
  cin >> N;
  vector<int> A(N);
  int i;
  for (i = 0; i < A.size(); i++) {
    cin >> A.at(i);
  }
  while (divide(A)) {
    for (i = 0; i < A.size(); i++) {
      A.at(i) /= 2;
    }
    count++;
  }
  cout << count << endl;
}
