#include <bits/stdc++.h>
#include <iostream>
using namespace std;

int main(){
  int N;
  cin >> N;
  vector<int> A(N);
  for(int i=0; i<N; i++)
  {
    cin >> A[i];
  }
  
  int count=0;
  
  for (int x : A) {
    if(X%2 == 0){
    x = x/2
    }
      count++
    
    if(X%2 != 0){
    break;
    }
  }
  
  cout << count << endl;
  return 0;
}
