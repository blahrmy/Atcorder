#include <bits/stdc++.h>
#include <iostream>
using namespace std;

int main(){
  int N;
  cin >> N;
  vector<int> A(N);  
  for(int i=0; i<N; i++){
    cin >> A[i];
  }

  int x = 0;
  int count = 0;
  
  for (int x : A){
    if(x%2 == 0){
      x = x/2;
    }
      count++;
    
    //else{
      //break;
    //}
  }
  
  cout << count << endl;
  return 0;
}
