#include <bits/stdc++.h>
#include <iostream>
using namespace std;

int main(){
  int N;
  cin >> N;
  vector<int> A(N);
  for(int i=0; i<N; i++)
  {
    cin >> A[i];
  }
  
  int count=0;
  //bool flag=true;

  for (int x : A) {
    if(X%2 == 0){
    x = x/2
    }

    if(X%2 != 0){
    break;
    }
    cout << count << endl;
  }


  //*
  while(true)
  {
    for(int i=0; i<N; i++)
    {
      if(A[i]%2 == 0)
      A[i] = A[i]/2;
      else
      {
        //false;
        break;
      }
    }
    if(true)
    count++;  //trueならcountが1ずつ増える（2で割れたってこと）
  }
  *//

  cout << count << endl;
  return 0;
}
