#include <bits/stdc++.h>
using namespace std;
int main(){
  int n;
  cin>>n;
  vector<int> d(n);        //vectorで餅をどんどんストックする
  for(int i=0; i<n; i++){
    cin>>d.at(i);
  }
  sort(d.begin(),d.end());  //ソートで小さい順に並び替え
  int c=0;
  int last=0;
  for(int i:d){            //範囲for文という。でもiをint定義してなくない？
    if(i!=last){           //lastは最新サイズ。どんどん大きくなり、同サイズでループがとまる
      c++;
      last=i;              //カウント後、lastは更新されていく（同サイズになるまでずっと）
    }
  }
  cout<<c<<endl;
}

//int iのiが定義されてなかったりするがこれでいいらしい
//すっきりしないのは自分がC++常識になれてないだけだと思う。継続すればそのうち慣れる。
