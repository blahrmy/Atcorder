#include <bits/stdc++.h>
using namespace std;
int main(){
  int n;
  cin>>n;
  vector<int> d(n);        //vectorで餅をどんどんストックする
  for(int i=0; i<n; i++){
    cin>>d.at(i);
  }
  sort(d.begin(),d.end());  //ソートで小さい順に並び替え
  int c=0;
  int last=0;
  for(int i:d){            //for (配列の要素の型 変数名 : 配列変数) 配列変数dからデータを変数iへコピーするらしい
    if(i!=last){           //lastは最後（最大サイズ）の鏡餅なのか？iでないものがlastの時にカウントする？意味不明
      c++;
      last=i;
    }
  }
  cout<<c<<endl;
}
